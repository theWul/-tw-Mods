	<RecipeDef ParentName="SurgeryAdvanced">
		<defName>InstallAdvancedBionicFinger</defName>
		<label>install advanced finger</label>
		<description>Installs an advanced bionic finger.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing advanced finger.</jobString>
		<workAmount>350</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>AdvancedBionicFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>AdvancedBionicFinger</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Finger</li>
		</appliedOnFixedBodyParts>
		<addsHediff>AdvancedBionicFinger</addsHediff>
	</RecipeDef>
     
