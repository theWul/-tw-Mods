<?xml version="1.0" encoding="utf-8" ?> 
<Defs>
	<RecipeDef ParentName="RecipeAdvancedProsthesis">
		<defName>CreateAdvancedBionicFinger</defName>
		<label>Upgrade bionic finger</label>
		<description>Upgrades a bionic finger with energized components.</description>
		<jobString>Upgrading bionic.</jobString>
		<products>
			<AdvancedBionicFinger>1</AdvancedBionicFinger>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicFinger</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>
     
