<?xml version="1.0" encoding="utf-8" ?> 
<Defs>
	<RecipeDef ParentName="RecipeBionicProsthesis">
		<defName>CreateBionicFinger</defName>
		<label>craft bionic finger</label>
		<description>Upgrades a prosthetic finger using plasteel and components.</description>
		<jobString>Crafting bionics.</jobString>
		<soundWorking>EPOEAssembling</soundWorking>
		<products>
			<BionicFinger>1</BionicFinger>
		</products>
		<recipeUsers>
			<li>TableBionics</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ProstheticFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ProstheticFinger</li>
				<li>Plasteel</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
     </RecipeDef>
     
