<?xml version="1.0" encoding="utf-8" ?> 
<Defs>
	<RecipeDef Abstract="True" Name="RecipeBasicProsthesis">
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<workSkill>Crafting</workSkill>
		<workAmount>2500</workAmount>
	</RecipeDef>

     
	<RecipeDef ParentName="RecipeBasicProsthesis">
		<defName>CreateProstheticFinger</defName>
		<label>craft simple prosthetic finger</label>
		<description>Crafts a prosthetic finger from steel and plasteel.</description>
		<jobString>Crafting prosthesis.</jobString>
		<soundWorking>EPOEAssembling</soundWorking>
		<products>
			<ProstheticFinger>1</ProstheticFinger>
		</products>
		<recipeUsers>
			<li>TableSimpleProsthetic</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
     </RecipeDef>
     
	<RecipeDef ParentName="RecipeBasicProsthesis">
		<defName>CreateProstheticToe</defName>
		<label>craft simple prosthetic toe</label>
		<description>Craft a prosthetic toe from steel and plasteel.</description>
		<jobString>Crafting prosthesis.</jobString>
		<soundWorking>EPOEAssembling</soundWorking>
		<products>
			<ProstheticToe>1</ProstheticToe>
		</products>
		<recipeUsers>
			<li>TableSimpleProsthetic</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
     </RecipeDef>

	<RecipeDef ParentName="SurgerySimple">
		<defName>InstallProstheticFinger</defName>
		<label>install prosthetic finger</label>
		<description>Installs a prosthetic finger.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing prosthetic finger.</jobString>
		<workAmount>350</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ProstheticFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>ProstheticFinger</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>LeftHandPinky</li>
			<li>RightHandPinky</li>
			<li>LeftHandRingFinger</li>
			<li>RightHandRingFinger</li>
			<li>LeftHandMiddleFinger</li>
			<li>RightHandMiddleFinger</li>
			<li>LeftHandIndexFinger</li>
			<li>RightHandIndexFinger</li>
			<li>LeftHandThumb</li>
			<li>RightHandThumb</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ProstheticFinger</addsHediff>
	</RecipeDef>
     
	<RecipeDef ParentName="SurgerySimple">
		<defName>InstallProstheticToe</defName>
		<label>install prosthetic toe</label>
		<description>Installs a prosthetic toe.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing prosthetic toe.</jobString>
		<workAmount>750</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ProstheticToe</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>ProstheticToe</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>LeftFootLittleToe</li>
			<li>RightFootLittleToe</li>
			<li>LeftFootFourthToe</li>
			<li>RightFootFourthToe</li>
			<li>LeftFootMiddleToe</li>
			<li>RightFootMiddleToe</li>
			<li>LeftFootSecondToe</li>
			<li>RightFootSecondToe</li>
			<li>LeftFootBigToe</li>
			<li>RightFootBigToe</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ProstheticToe</addsHediff>
	</RecipeDef>


</Defs>